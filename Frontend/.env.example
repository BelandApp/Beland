# =========== PAYPHONE CONFIGURATION ===========
EXPO_PUBLIC_PAYPHONE_TOKEN=your_payphone_token_here
EXPO_PUBLIC_PAYPHONE_STOREID=your_store_id_here

# =========== AUTH0 CONFIGURATION ===========
EXPO_PUBLIC_AUTH0_DOMAIN=your-auth0-domain.us.auth0.com
EXPO_PUBLIC_AUTH0_WEB_CLIENT_ID=your_web_client_id_here
EXPO_PUBLIC_AUTH0_MOBILE_CLIENT_ID=your_mobile_client_id_here

# =========== BACKEND CONFIGURATION ===========
EXPO_PUBLIC_API_URL=https://api.yourdomain.com
EXPO_PUBLIC_USE_DEMO_MODE=true

# =========== INSTRUCCIONES ===========
# 1. Copia este archivo como .env
# 2. Reemplaza los valores con tus credenciales reales
# 3. El archivo .env está en .gitignore por seguridad
# 4. EXPO_PUBLIC_USE_DEMO_MODE=true para desarrollo sin backend
# 5. EXPO_PUBLIC_USE_DEMO_MODE=false para producción con backend real